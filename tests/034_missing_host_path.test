#!/bin/bash

#
# Test failure scenarios in the config file
#

. ./common.sh

cat >$CONFIG <<EOF
file-resource TEST {
}

task complete {
	on-resource TEST { raw_write(0) }
}
EOF

echo "Expecting error from fwup..."
if $FWUP_CREATE -c -f $CONFIG -o $FWFILE; then
    echo "This should have failed"
    exit 1
fi

cleanup
